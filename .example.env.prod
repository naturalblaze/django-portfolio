# Production environment variables - .env
DJANGO_ENV=prod
DJANGO_DEBUG=False
DJANGO_SECRET_KEY=super-secret-prod-key # CHANGE-ME
DJANGO_WORDCLOUD=secret agent
DJANGO_ALLOWED_HOSTS=domain-name # CHANGE-ME
DJANGO_CSRF_TRUSTED_ORIGINS=https://domain-name # CHANGE-ME
DJANGO_SESSION_COOKIE_NAME=__Secure-sessionid
DJANGO_CSRF_COOKIE_NAME=__Secure-csrftoken
DJANGO_DATABASE_URL=postgres://<POSTGRES_USER>:<POSTGRES_PASSWORD>@<POSTGRES_HOST>:<POSTGRES_PORT>/<POSTGRES_DB>
